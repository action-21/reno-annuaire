{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "lib": ["DOM", "ESNext", "esnext"],
    "moduleResolution": "Node",
    "incremental": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "allowJs": true
  },
}
